<%include _header%>

<h1>Registro</h1>
<form action="/registro_equipo" method="post">
<div class="form-group">
<input name="description" type="text" class="form-control" 
    placeholder="Descripcion" required autofocus/>
</div>
<div class="form-group">
<input name="defense" type="number" class="form-control"
    placeholder="Defensa" required/>
 </div>
 <div class="form-group">
<input name="category" type="text" class="form-control"
    placeholder="Categoria" />
 </div>
 <div class="form-group">
<input name="weight" type="number" class="form-control"
    placeholder="peso" />
 </div>
 <input type="submit" value="Registrar equipo" class="btn btn-primary btn-block"/>
 </form>

 <%include _footer%>