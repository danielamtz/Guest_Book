<%include header%>
<h2>Crea una nueva entrada</h2>
<form method="post" role="form">
<div class="form-group">
    <label for="title">titulo</label> 
   <input type="text" class="form-control" id="title" name="title" placeholder="Escribe un titulo" required/>

</div>
<div class="form-group">
    <label for="body">mensaje</label>
    <textarea class="form-control" id="body" name="body" placeholder="escribir un mensaje"rows="3" required></textarea>
</div>
<div class="form-group">
<input type="submit" class="btn btn-primary" value="Aceptar">

</div>

</form>

<%include footer%>